<link
	rel="stylesheet"
	href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css"
	href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
/>
<section class="sign-up-box">
	<header>
		<a href="login_page.html">
			<h1>ticket<span>Scout</span></h1>
			<img id="logo-img" src="/assets/ticketScoutLogo.svg" alt="" />
		</a>
	</header>
	<form action="/join-store" method="POST">
		<label for="sign-up-fullname">Full name</label>
		<div class="input-wrap">
			<input
				autocomplete="no"
				id="sign-up-fullname"
				type="fullname"
				name="fullname"
				placeholder="John Smith"
				required
			/>
			<i class="fa fa-user"></i>
		</div>
		<br />
		<label for="sign-up-email">Email</label>
		<div class="input-wrap">
			<input
				autocomplete="no"
				id="sign-up-email"
				type="email"
				name="email"
				placeholder="hello@example.com"
				required
			/>
			<i class="fa fa-mail-forward"></i>
		</div>
		<br />
		<label for="sign-up-psw">Password</label>
		<div class="input-wrap">
			<input
				id="sign-up-psw"
				type="password"
				name="password"
				placeholder="&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;"
				required
			/>
			<i class="fa fa-lock"></i>
		</div>
		<br />
		<label for="sign-up-psw-confirm">Confirm password</label>
		<div class="input-wrap">
			<input
				id="sign-up-psw-confirm"
				type="password"
				name="passwordConfirm"
				placeholder="&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;"
				required
			/>
			<i class="fa fa-check-circle"></i>
		</div>
		<br />
		<div class="link-wrap">
			<button id="sign-up-btn" type="submit">Sign Up</button>
		</div>
	</form>
</section>
<section class="graphic-box">
	<div class="text-wrap">
		<h1>
			Welcome to <span class="first">ticket</span
			><span class="second">Scout</span>
		</h1>
		<h2 id="graphic-box-text">Enter in your details to start your day!</h2>
	</div>
</section>
